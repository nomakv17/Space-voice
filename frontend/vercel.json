{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "framework": "nextjs",
  "buildCommand": "npm run build",
  "installCommand": "npm install",
  "devCommand": "npm run dev",
  "regions": ["iad1"],
  "redirects": [
    {
      "source": "/dashboard/:path*",
      "destination": "https://dashboard.spacevoice.ai/dashboard/:path*",
      "has": [{ "type": "host", "value": "spacevoice.ai" }],
      "permanent": false
    },
    {
      "source": "/dashboard/:path*",
      "destination": "https://dashboard.spacevoice.ai/dashboard/:path*",
      "has": [{ "type": "host", "value": "www.spacevoice.ai" }],
      "permanent": false
    },
    {
      "source": "/login",
      "destination": "https://dashboard.spacevoice.ai/login",
      "has": [{ "type": "host", "value": "spacevoice.ai" }],
      "permanent": false
    },
    {
      "source": "/login",
      "destination": "https://dashboard.spacevoice.ai/login",
      "has": [{ "type": "host", "value": "www.spacevoice.ai" }],
      "permanent": false
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        },
        {
          "key": "X-Frame-Options",
          "value": "DENY"
        },
        {
          "key": "X-XSS-Protection",
          "value": "1; mode=block"
        },
        {
          "key": "Referrer-Policy",
          "value": "strict-origin-when-cross-origin"
        }
      ]
    }
  ]
}
